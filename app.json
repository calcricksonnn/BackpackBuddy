{
  "expo": {
    "name": "BackpackBuddy",
    "slug": "backpackbuddy",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.PNG",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yourcompany.backpackbuddy"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.PNG",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.yourcompany.backpackbuddy"
    },
    "web": {
      "favicon": "./assets/icon.PNG"
    },
    "plugins": ["expo-notifications"]
  }
}